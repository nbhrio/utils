{
  "name": "@nbhr/utils",
  "version": "0.0.29",
  "description": "",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./fs": {
      "import": "./dist/fs/fs.js"
    },
    "./debugger": {
      "import": "./dist/debugger/debugger.js"
    }
  },
  "typesVersions": {
    "*": {
      "fs": [
        "./dist/fs.d.ts"
      ],
      "debugger": [
        "./dist/debugger.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:clean": "rimraf dist && npm run build",
    "test": "c8 --reporter=lcov --reporter=text --all --include src uvu -r sucrase/register tests",
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts src/fs/fs.ts src/debugger/debugger.ts --dts --format esm --out-dir dist --no-splitting",
    "release": "release-it --no-npm.publish --github.release"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexander Niebuhr <tech@nbhr.io>",
  "license": "ISC",
  "devDependencies": {
    "@alexanderniebuhr/style": "1.0.13",
    "@types/node": "16.11.7",
    "auto-changelog": "2.3.0",
    "c8": "7.10.0",
    "release-it": "14.11.6",
    "rimraf": "3.0.2",
    "sucrase": "3.20.3",
    "ts-standard": "10.0.0",
    "tsup": "5.6.0",
    "typescript": "4.4.4",
    "uvu": "0.5.2"
  }
}
