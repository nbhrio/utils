{
  "name": "@nbhr/utils",
  "version": "0.0.27",
  "description": "",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./fs": {
      "require": "./dist/fs/fs.js",
      "import": "./dist/fs/fs.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "fs": [
        "./dist/fs.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:clean": "rimraf dist && npm run build",
    "test": "c8 --reporter=lcov --reporter=text --all --include src uvu -r sucrase/register tests",
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts src/fs/fs.ts --dts --format cjs,esm --out-dir dist",
    "build:old": "esbuild src/index.ts src/fs/index.ts --bundle --outdir=dist --platform=node --format=esm --out-extension:.js=.mjs --splitting && esbuild src/index.ts src/fs/index.ts --bundle --outdir=dist --platform=node --format=cjs && npm run types",
    "types": "npx tsc src/index.ts --declaration --emitDeclarationOnly --outdir dist/types && npx tsc src/fs/index.ts --declaration --emitDeclarationOnly --outdir dist/fs/types",
    "release": "release-it --no-npm.publish --github.release"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexander Niebuhr <45965090+alexanderniebuhr@users.noreply.github.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "15.0.2",
    "auto-changelog": "2.2.1",
    "c8": "7.7.2",
    "release-it": "14.6.1",
    "rimraf": "3.0.2",
    "sucrase": "3.18.1",
    "ts-standard": "10.0.0",
    "tsup": "4.10.1",
    "typescript": "4.2.4",
    "uvu": "0.5.1"
  }
}
